{
	"name": "greenchase",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"prepare": "husky install",
		"precommit": "lint-staged"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix"
		],
		"*.{html,js}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"next": "12.1.6",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.7",
		"eslint": "8.17.0",
		"eslint-config-next": "12.1.6",
		"eslint-config-prettier": "^8.5.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.1",
		"postcss": "^8.4.14",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.1.3"
	}
}
